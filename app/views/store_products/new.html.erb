<h1>New Store Product</h1>

<%= render 'form', store_product: @store_product %>

<%= @products.each do |product| %>
<%= product.title %>
<% end %>

<h1> Associated Store Products </h1>
<%= @store_products.each do |store_product| %>
<%= store_product.id %>
<td>
  <%= form_for(store_product, :html => { :method => :delete, :remote => true }) do |f| %>
    <%= f.button "Delete", data: {confirm: "Are you sure?"}, class: "form-control" %>
  <% end %>
</td>

<% end %>
