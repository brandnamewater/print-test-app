<h1>All Orders</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @store_orders.each do |order| %>
      <tr>
        <td><%= order %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<h1>All Orders That Match Associated Products</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @match_store_orders.each do |order| %>
      <tr>
        <td><%= order %></td>
      </tr>
    <% end %>
  </tbody>
</table>
