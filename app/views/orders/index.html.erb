      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @store_orders.each do |order| %>
      <tr>
        <td><%= order.line_items %></td>
      </tr>
    <% end %>
  </tbody>
</table>



<br>
<h1>All Orders That Match Associated Products</h1>
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @store_orders.each do |order| %>
      <tr>
        <td><%= order.id %> </td>
        <td><%= order.line_items.count %>
        <% order.line_items.each do |line| %>
          <td><%= line.product_id %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

